/&gt;&lt;br /&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr&gt;&lt;td align="left" width="33%"&gt;&lt;a href="http://www.ssfpa.se/uk/artiklar2002.html"&gt;&lt;img alt="adoranten 2002" src="http://www.ssfpa.se/images/artiklar2002.gif" width="100" /&gt;&lt;/a&gt;&lt;br /&gt;&lt;span&gt;Articles 2002&lt;/span&gt;&lt;br /&gt;&lt;br /&gt;&lt;/td&gt;&lt;td align="left" width="33%"&gt;&lt;br /&gt;&lt;/td&gt;&lt;/tr&gt;&lt;/tbody&gt;&lt;/table&gt;&lt;/td&gt;&lt;td align="left" width="33%"&gt;&lt;br /&gt;&lt;/td&gt;&lt;td align="left" width="33%"&gt;&lt;br /&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr&gt;&lt;td align="left" width="33%"&gt;&lt;br /&gt;&lt;/td&gt;&lt;td align="left" width="34%"&gt;&lt;br /&gt;&lt;/td&gt;&lt;td align="left" width="33%"&gt;&lt;br /&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr&gt;&lt;td align="left" width="33%"&gt;&lt;br /&gt;&lt;/td&gt;&lt;td align="left" width="33%"&gt;&lt;br /&gt;&lt;/td&gt;&lt;td align="left" width="34%"&gt;&lt;br /&gt;&lt;/td&gt;&lt;/tr&gt;&lt;/tbody&gt;&lt;/table&gt;&lt;/blockquote&gt;&lt;div style="text-align: center;"&gt;See the full &lt;a href="http://ancientworldonline.blogspot.com/2009/10/alphabetical-list-of-open-access.html"&gt;List of Open Access Journals in Ancient Studies&lt;/a&gt;&lt;/div&gt;&lt;br /&gt;&lt;span style="font-size: 85%;"&gt;&lt;/span&gt;</content>
